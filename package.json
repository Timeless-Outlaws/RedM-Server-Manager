{
  "name": "redm-server-manager",
  "description": "A CLI server manager written for the dockerized FiveM server.",
  "version": "0.0.8",
  "author": "Patrick Henninger",
  "bin": {
    "rsm": "./bin/run"
  },
  "bugs": "https://github.com/bumbummen99/RedM/issues",
  "dependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@types/glob": "^7.2.0",
    "@types/is-git-url": "^1.0.0",
    "@types/jsdom": "^16.2.14",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.12",
    "@types/node-fetch": "^2.6.2",
    "@types/tar": "^6.1.1",
    "cli-ux": "^6.0.9",
    "glob": "^7.2.0",
    "globby": "^11",
    "is-git-url": "^1.0.0",
    "jsdom": "^19.0.0",
    "node-fetch": "^2.6.1",
    "oclif": "^3.2.24",
    "simple-git": "^2.48.0",
    "tar": "^4.4.19",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.1.1",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-plugin-mocha": "^10.0.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "mocha": "^9"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/bumbummen99/RedM",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "rsm",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "bumbummen99/RedM",
  "scripts": {
    "build": "rm -rf lib && tsc -b",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
